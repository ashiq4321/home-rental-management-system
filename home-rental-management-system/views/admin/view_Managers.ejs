<!DOCTYPE html>
<html>

<head>
	<title>Managers</title>
</head>

<body>
	<div>
		<h2>Managers</h2>
	</div>

	<a href="/admin">Back</a>|
	<a href="/logout">Logout</a><br><br>
	<a href="/admin/profile">Edit Profile</a><br>
	<a href="/admin/addNewManager">Add New Manager</a><br>
	<a href="/admin/assignManager">Assign Manager </a><br>
	<a href="/admin/view_Managers">Managers</a><br>
	<a href="/admin/view_Owners">Owners</a><br>
	<a href="/admin/view_Customers">Customers</a><br>
	<a href="/admin/view_Rented">Rented Apartments</a><br>
	<a href="/admin/view_Available">Available Apartments</a><br>

	<table border="1">
		<tr>
			<td>First Name</td>
			<td>Last Name</td>
			<td>USERNAME</td>
			<td>Email</td>
			<td>Phone</td>
			<td>Division</td>
			<td>Area</td>
			<td>Nid</td>
			<td>status</td>
		</tr>
		<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].fname %></td>
			<td><%= userlist[i].lname %></td>
			<td><%= userlist[i].username %></td>
			<td><%= userlist[i].email %></td>
			<td><%= userlist[i].phone %></td>
			<td><%= userlist[i].division %></td>
			<td><%= userlist[i].area %></td>
			<td><%= userlist[i].nid %></td>
			<td><%= userlist[i].status %></td>
			<td>
				<a href="/admin/view_Managers/<%= userlist[i].username %>">View Profile </a>
			</td>
		</tr>
		<%  } %>
	</table>

</body>

</html>